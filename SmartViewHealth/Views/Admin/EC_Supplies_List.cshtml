@model List<SmartViewHealth.Models.Equipment_EC_Supplies>

@if (Model.Count > 0)
{
    <ul class="list-group" style="margin: 5px;">
        @foreach (var item in Model)
        {
            <a href="javascript: ShowEditSuppliesModal('@item.Id', '@item.Supply_Name', '@item.Supply_Qty', '@item.row_sta_cd');" class="list-group-item list-group-item-action flex-column align-items-start">
                <div class="d-flex w-100 justify-content-between">
                    <h5 class="mb-1">@item.Supply_Name</h5>
                    <small>Qty: @item.Supply_Qty</small>
                </div>
            </a>
        }
    </ul>
}
else
{
    <h6 class="text-danger">There are currently no supplies.</h6>
}